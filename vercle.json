{
    "version": 2,
    "builds": [
    {
    "scr": "Blog/wsgi.py",
    "user": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    },
    {
    "scr": "build_files.sh",
    "user": "@vercel/static-build",
    "config": {
    "distDir": "staticfiles_build"
     }
    }
    ],
    "routes": [
    {
    "scr": "/static/(.*)",
    "dest": "/static/$1"
    },
    {
    "scr": "/(.*)",
    "dest": "Blog/wsgi.py"
    }
    ]
}